{
  "name": "vscode-hentai",
  "displayName": "vscode-hentai",
  "description": "code and display hentai image",
  "version": "0.1.0",
  "bugs": {
    "url": "https://github.com/Himself65/vscode-hentai/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Himself65/vscode-hentai.git"
  },
  "homepage": "https://github.com/Himself65/vscode-hentai",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "configuration": {
    "title": "Hentai Mode",
    "properties": {
      "hentai.enabled": {
        "default": false,
        "type": "boolean",
        "description": "Enable to activate Hentai MODE!!!"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "jest -i"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/vscode": "^1.36.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-serializer-html": "^7.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  }
}
